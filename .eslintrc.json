{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint" ,
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "error",
    "no-await-in-loop": "warn",
    "no-duplicate-case": "warn",
    "no-duplicate-imports": "warn"
  }
}
